{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"fastapi\"}[5m])) by (method)",
          "legendFormat": "{{ method }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{service=\"fastapi\",status=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"fastapi\"}[5m]))",
          "legendFormat": "Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percentunit", "max": 1 }
      }
    },
    {
      "title": "Response Time (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{service=\"fastapi\"}[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{service=\"fastapi\"}[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{service=\"fastapi\"}[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum(http_connections_active{service=\"fastapi\"})",
          "legendFormat": "Active"
        }
      ]
    },
    {
      "title": "Uptime",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 8 },
      "targets": [
        {
          "expr": "avg(up{service=\"fastapi\"})",
          "legendFormat": "Uptime"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percentunit" }
      }
    },
    {
      "title": "Requests by Endpoint",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "topk(10, sum(rate(http_requests_total{service=\"fastapi\"}[5m])) by (endpoint, method, status))",
          "legendFormat": "{{ method }} {{ endpoint }} ({{ status }})",
          "format": "table",
          "instant": true
        }
      ]
    },
    {
      "title": "Pod CPU Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"imagineai\"}[5m])) by (pod)",
          "legendFormat": "{{ pod }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short" }
      }
    },
    {
      "title": "Pod Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "sum(container_memory_working_set_bytes{namespace=\"imagineai\"}) by (pod)",
          "legendFormat": "{{ pod }}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["imagineai", "api", "overview"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "ImagineAI - Application Overview",
  "uid": "imagineai-app-overview"
}
